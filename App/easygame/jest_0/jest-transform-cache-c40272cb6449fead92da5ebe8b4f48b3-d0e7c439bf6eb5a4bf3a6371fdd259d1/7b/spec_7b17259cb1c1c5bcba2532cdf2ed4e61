var _interopRequireDefault = require("@babel/runtime/helpers/interopRequireDefault");

var _react = _interopRequireDefault(require("react"));

var _enzyme = require("enzyme");

var _verifierDonnees = _interopRequireDefault(require("../components/Accueil/verifierDonnees"));

describe('Test la validité des données entrées par l\'utilisateur', function () {
  var initialData = {
    email: 'manuelle.ndamtang@yahoo.com',
    password: 'manou'
  };
  test('Detecte un email et mot de passe manquants', function () {
    expect((0, _verifierDonnees.default)('', '')).toBeFalsy();
  });
  test('Detecte un email introduit et mot de passe manquant', function () {
    expect((0, _verifierDonnees.default)(initialData.email, '')).toBeFalsy();
  });
  test('Detecte un email manquant et mot de passe introduit', function () {
    expect((0, _verifierDonnees.default)('', initialData.password)).toBeFalsy();
  });
  test('un email et mot de passe introduits', function () {
    expect((0, _verifierDonnees.default)(initialData.email, initialData.password)).toBeTruthy();
  });
});
